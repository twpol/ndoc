<?xml version="1.0" ?>
<project name="NDoc.Test" default="compile">
	<target name="compile">
		<csc target="library" output="${current.bin.dir}/${nant.project.name}.dll" debug="false"
			define="${current.build.defines}" doc="${current.bin.dir}/${nant.project.name}.xml">
			<sources>
				<includes name="**/*.cs" />
			</sources>
			<references basedir="${nant.settings.currentframework.frameworkassemblydirectory}">
				<includes name="System.dll" />
				<includes name="System.Xml.dll" />
			</references>
		</csc>
	</target>
</project>
